generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Day {
  id          String   @id @default(cuid())
  date        String   @unique // 2022-01-22
  totalOpened Int
  totalClosed Int
  createdAt   DateTime @default(now())
}

model DayPR {
  id          String   @id @default(cuid())
  date        String   @unique // 2022-01-22
  totalOpened Int
  totalClosed Int
  createdAt   DateTime @default(now())
}
